                                "filterValues": [
                                    {
                                        "propertyName": "automation_queue",
                                        "operator": "EQ",
                                        "value": "queue"
                                    }
                                ]
                            }
                        }
                    ]
                },
                "properties": [
                    "dealname",
                    "call_outcome",
                    "follow_up_date",
                    "automation_queue",
                    "property_link"
